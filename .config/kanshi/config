profile Dell_5570 {
	output "Samsung Electric Company LS34A650U H1AK500000" mode 3440x1440 position 0,0 enable
	output "Samsung Electric Company S22R35x H4TR501175" position 3440,0 enable
	output "LG Display 0x06B3 Unknown" mode 1920x1200 position 3440,1080 enable
	exec swaymsg [workspace=1] move workspace to output \"Samsung Electric Company LS34A650U H1AK500000\"
	exec swaymsg [workspace=2] move workspace to output \"Samsung Electric Company LS34A650U H1AK500000\"
	exec swaymsg [workspace=3] move workspace to output \"Samsung Electric Company LS34A650U H1AK500000\"
	exec swaymsg [workspace=4] move workspace to output \"Samsung Electric Company LS34A650U H1AK500000\"
	exec swaymsg [workspace=5] move workspace to output \"Samsung Electric Company LS34A650U H1AK500000\"
	exec swaymsg [workspace=8] move workspace to output eDP-1
	exec swaymsg [workspace=9] move workspace to output eDP-1
	exec swaymsg [workspace=music] move workspace to output eDP-1
}

